{
    "name": "laravelgpt/VscodeLaravel",
    "description": "A Laravel 12 package that functions as a VS Code web-based extension with Git features",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shamim Hossain",
            "email": "shamimstack@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^10.6",
        "phpunit/phpunit": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelGpt\\Vscode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelGpt\\Vscode\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelGpt\\Vscode\\VscodeLaravelGitServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
